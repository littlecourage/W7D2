<h1>Add A Band!</h1>

<form action="<%= bands_url %>" method = "POST">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <input type="text"
    name="band[name]"
    id="band_name"
    value="<%= @band.name %>">
  <br>
  <input type="submit">

</form>